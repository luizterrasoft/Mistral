  object Query1: TQuery
    BeforeOpen = Query1BeforeOpen
    DatabaseName = 'VENDAS'
    SQL.Strings = (
      'Select ch_caix as Terminal,'
      '       ch_cpf  as CPF'
      '      ,Ch_banc as Banco'
      '      ,ch_agen As Age'
      '      ,ch_cont as Conta'
      '      ,ch_nume as Numero'
      '      ,ch_valo as Valor'
      'from Cheques C'
      
        'left join vendas on (ve_caix = c.ch_caix and ve_loja = c.ch_loja' +
        ' and ve_codi = c.ch_vend)'
      'where c.ch_loja    =:loja'
      'and    (c.ch_caix    =:Terminal or :Terminal = 0)'
      'and   c.ch_sangria =:sang'
      'and   ve_data      =:data')
    Left = 304
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'loja'
